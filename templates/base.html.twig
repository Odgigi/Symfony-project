    <body>
        <!-- Google Tag Manager (noscript) -->
        <noscript><iframe src="https://www.googletagmanager.com/ns.html?id=GTM-MM2XGZQ"
        height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
        <!-- End Google Tag Manager (noscript) -->
        {% include('navbar.html.twig') %}
        {% block body %}
        <div class="container marketing mx-5">
            {% block content %}
            {% endblock %}
        </div>
        {% endblock %}

        <footer class="container-fluid">
            <div class="row text-center bg-dark">
                <div class="col-5 my-auto pt-2">
                    <h5>Vers</h5>
                    <div class="dropdown-divider"></div>
                    <p><a href="{{ path('home') }}">Haut de la page</a></p>
                    <p><a href="#">FAQ</a></p>
                    <p><a href="{{ path('c_g_u') }}">CGU</a></p>
                    {# {% if is_granted('ROLE_ADMIN') %}{% endif %} le lien ne s'affichera que si je suis connectée et que j'ai le rôle admin #}
                    <p><a href="{{ path('admin_annonce_index') }}">Admin</a></p>
                    
                </div>
                <div class="col-2 my-auto">
                    <img src="{{ asset('assets/brand/logo8.png') }}" alt="logo8 Fétiche" class="w-100">
                </div>
                <div class="col-5 my-auto pt-2">
                    <h5>Coordonnées</h5>
                    <div class="dropdown-divider"></div>
                    <p>{{ contact_email }}</p>
                    <p>{{ web_site }}</p>
                    <p><i class="fab fa-instagram"></i> {{ compte_instagram }}</p>
                    <p><i class="fab fa-pinterest"></i> {{ compte_pinterest }}</p>
                </div>
            </div>
            <div class="row" id="copyright">
                <div class="col-12 text-center">
                    <p>{{ copyright }}</p>
                </div>
            </div>
        </footer>
        
        {% block javascript %}{% endblock %}
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
        
